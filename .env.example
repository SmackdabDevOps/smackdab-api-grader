# PostgreSQL Connection String
DATABASE_URL=postgresql://user:password@host:port/database

# Or individual PostgreSQL settings
PGHOST=localhost
PGPORT=5432
PGUSER=grader
PGPASSWORD=your_password
PGDATABASE=api_grader

# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# Rate Limiting
RATE_LIMIT=100

# API Keys (JSON format)
# Format: {"api_key": {"teamId": "team_name", "userId": "user_id"}}
API_KEYS={"sk_dev_123": {"teamId": "dev-team", "userId": "dev-user"}}

# Template Path (in container)
TEMPLATE_PATH=/app/.claude/templates/MASTER_API_TEMPLATE_v3.yaml